{
  "name": "my-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p validate:dev sapper:dev",
    "build": "run-s sapper:build validate",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "sapper:dev": "sapper dev",
    "sapper:build": "cross-env NODE_ENV=production sapper build --legacy",
    "sapper:export": "cross-env NODE_ENV=production sapper export --legacy",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "polka": "^0.5.2",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.28.1",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.8",
    "svelte": "^3.25.1",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.3.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
